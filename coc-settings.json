{
  "list.source.files.excludePatterns": [
    "**/node_modules/**",
    "**/node_modules/**/.*",
    "**/node_modules/**/.*/**"
  ],
  "tabnine.priority": 50,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "scss",
    "typescript",
    "javascript",
    "html",
    "json"
  ]
}
